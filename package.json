{
  "name": "@peggyjs/eslint-config",
  "version": "3.0.6",
  "description": "Lint rules for peggyjs projects",
  "main": "index.js",
  "keywords": [
    "peggy",
    "eslint"
  ],
  "scripts": {
    "lint": "eslint . --ext js",
    "missing": "npm run missing:js && npm run missing:ts",
    "missing:js": "eslint-find-rules --verbose -u index.js",
    "missing:ts": "eslint-find-rules --verbose --no-core -u ts.js",
    "test": "eslint test --ext js,ts"
  },
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "repository": "peggyjs/peggyjs-eslint-config",
  "peerDependencies": {
    "@stylistic/eslint-plugin": "~1",
    "@typescript-eslint/eslint-plugin": "~6",
    "@typescript-eslint/parser": "~6",
    "eslint": "~8",
    "typescript": "~5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    }
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-find-rules": "4.1.0",
    "typescript": "^5.3.3"
  }
}
