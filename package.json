{
  "name": "@peggyjs/eslint-config",
  "version": "3.0.1",
  "description": "Lint rules for peggyjs projects",
  "main": "index.js",
  "keywords": [
    "peggy",
    "eslint"
  ],
  "scripts": {
    "lint": "eslint . --ext js",
    "missing": "npm run missing:js && npm run missing:ts",
    "missing:js": "eslint-find-rules --verbose -u index.js",
    "missing:ts": "eslint-find-rules --verbose -u ts.js | grep -v https://eslint.org/docs/rules/"
  },
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "repository": "peggyjs/peggyjs-eslint-config",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "~5",
    "@typescript-eslint/parser": "~5",
    "eslint": "~8",
    "typescript": "~5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-find-rules": "4.1.0",
    "typescript": "^5.1.3"
  }
}
